{
  "name": "tiktok-content-manager",
  "version": "1.0.0",
  "description": "TikTok content management and auto-publishing system with OAuth",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "create-user": "node scripts/createUser.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "tiktok",
    "content-management",
    "oauth",
    "social-media",
    "automation"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "sequelize": "^6.32.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "axios": "^1.5.0",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "express-session": "^1.17.3",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.2"
